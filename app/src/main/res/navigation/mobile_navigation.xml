<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.water11.ui.home.HomeFragment"
        android:label="@string/title_water"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.water11.ui.dashboard.DashboardFragment"
        android:label="@string/title_rink"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.water11.ui.notifications.NotificationsFragment"
        android:label="@string/title_contact"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_service"
        android:name="com.example.water11.ui.Service.ServiceFragment"
        android:label="@string/title_service"
        tools:layout="@layout/service_fragment" />

    <fragment
        android:id="@+id/navigation_reservoir"
        android:name="com.example.water11.ui.Reservoir.ReservoirFragment"
        android:label="@string/title_reservoir"
        tools:layout="@layout/reservoir_fragment" />

</navigation>